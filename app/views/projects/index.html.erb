<h1 id="page_title">Projects Built</h1>
<div id="posts_wrapper" class="skinny_wrapper">
  <% @projects.each do |project| %>
    <div class="post">
        <p class="date"><%= project.created_at.strftime('%A, %B %d') %></p>
        <h2><a href=""><%= link_to project.title, project %></a></h2>
        <hr>
    </div>

    <% if user_signed_in? %>
      <div id="admin_links">
          <%= link_to 'Create New Project', new_project_path %>
      </div>
    <% end %>

  <% end %>
</div>